<% include ./partials/header %>

<h1> Track your bills here! </h1>

<div class='name'>
<% person.forEach(person => { %> 
 <h2> <%= person.name %> </h2>
   <% }) %>
</div>
<div class="my-bills">
<% bill.forEach(bill => { %>
  <li>Rent: <%= bill.rent %> </li>
  <li>Electric: <%= bill.electric %></li>
  <li>Car: <%= bill.car %></li>
  <li>Insurance: <%= bill.insurance %></li>
  <li>Food: <%= bill.food %></li>
  <li>Other: <%= bill.other %>s</li>
  
 <% }) %>

 </div>